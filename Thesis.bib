@article{ingramInterpretationChildrenViews2013,
  title = {Interpretation of Children's Views by Educational Psychologists: Dilemmas and Solutions},
  shorttitle = {Interpretation of Children's Views by Educational Psychologists},
  author = {Ingram, Rachel},
  year = {2013},
  month = dec,
  journal = {Educational Psychology in Practice},
  volume = {29},
  number = {4},
  pages = {335--346},
  publisher = {{Routledge}},
  issn = {0266-7363},
  doi = {10.1080/02667363.2013.841127},
  abstract = {This paper considers how listening to children's views and taking account of them is understood in different ways by educational psychologists. Rationales for listening to children include (1) supporting their rights to express views, (2) therapeutic benefits of participation, and either (3) valuing their views as insightful contributions to problem-solving or (4) considering them as contributory factors to the problem. This creates dilemmas about interpretation on different occasions, and a range of theoretical stances are discussed in terms of how far they can support robust decision-making about this interpretation, and a practical way forward is suggested using principles based on critical realism.},
  keywords = {children,educational psychologists,views,voice},
  annotation = {\_eprint: https://doi.org/10.1080/02667363.2013.841127},
  file = {C\:\\Users\\paddy\\Zotero\\storage\\7U2CTEF7\\Ingram - 2013 - Interpretation of children’s views by educational .pdf;C\:\\Users\\paddy\\Zotero\\storage\\JSVR88VG\\02667363.2013.html}
}





@article{smillieEducationalPsychologistsPractice2020,
  title = {Educational Psychologists' Practice: Obtaining and Representing Young People's Views},
  shorttitle = {Educational Psychologists' Practice},
  author = {Smillie, Ian and Newton, Meryl},
  year = {2020},
  month = jul,
  journal = {Educational Psychology in Practice},
  volume = {36},
  number = {3},
  pages = {328--344},
  publisher = {{Routledge}},
  issn = {0266-7363},
  doi = {10.1080/02667363.2020.1787955},
  abstract = {Many studies have focused on the importance of the participation of children and young people (CYP) in their education, although fewer papers discuss the practical aspects of seeking their views and fewer again on representing them. A mixed method approach was used to collect information regarding educational psychologists' (EPs') practice in obtaining and representing CYP's views. A questionnaire (n = 73) was used to gather data from local authority educational psychology services (EPSs) across Wales and eight participants who completed the questionnaire took part in an interview to explore their views in more depth. Using thematic analysis, the major themes constructed included ``accessing a true representation of CYP's views'', ``gaining CYP's views empowers them'' and ``child-centred practice''. The implications of the present study for the role of educational psychologists are discussed, together with future directions for research and the limitations of the present study.},
  keywords = {Educational Psychologists,educational psychology reports,interpreting children and young people’s views,obtaining and recording children and young people’s views,pupil voice},
  annotation = {\_eprint: https://doi.org/10.1080/02667363.2020.1787955},
  file = {C\:\\Users\\paddy\\Zotero\\storage\\76GTIYQ9\\Smillie y Newton - 2020 - Educational psychologists’ practice obtaining and.pdf;C\:\\Users\\paddy\\Zotero\\storage\\8DR8JNE5\\02667363.2020.html}
}





